{
  "name": "that-graphql-hype",
  "version": "1.0.0",
  "description": "Just experimental repo to try work with GraphQL and FabricUI, practice TypeScript and maybe complete Kiwi task",
  "author": "@lspdv",
  "license": "MIT",
  "keywords": [
    "graphQL",
    "react"
  ],
  "pre-commit": "lint:staged",
  "lint-staged": {
    ".{ts,tsx}": [
      "tslint --fix",
      "prettier --parser typescript --write",
      "git add"
    ]
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "lint:staged": "lint-staged"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.1.9",
    "apollo-client": "^2.2.5",
    "apollo-link-http": "^1.5.1",
    "graphql": "^0.12.0",
    "graphql-tag": "^2.8.0", 
    "office-ui-fabric-react": "^5.55.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts-ts": "2.13.0"
  },
  "devDependencies": {
    "@types/graphql": "^0.12.4",
    "@types/jest": "^22.1.3",
    "@types/node": "^9.4.6",
    "@types/react": "^16.0.38",
    "@types/react-dom": "^16.0.4",
    "typescript": "^2.7.2",
    "lint-staged": "^7.0.0",
    "pre-commit": "^1.2.2"
  }
}
